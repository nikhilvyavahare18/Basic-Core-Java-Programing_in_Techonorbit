class Animal
{
  public void eat()
  {
    System.out.println("eating..!!!!!!!!!");
  }
  public void  sleep()
  {
    System.out.println("sleep..!!!!!!!!!");
  }
}
class Dog extends Animal
{
  public void  Bark()
  {
  System.out.println("Barking..!!!!!!!!!");
  }
  public static void main()String []args)
  {
    Dog d = new Dog ();
    d.eat();
    d.sleep();
    d.Bark();
  }
}
